{
  "name": "tcomponent",
  "version": "1.0.0",
  "description": "Reusable function for rapid react development",
  "author": "aldoetobex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/telescoope/tcomponent.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --css-modules false --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --css-modules false --no-compress --format modern,cjs",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "gh-pages": "3.2.3",
    "microbundle-crl": "0.13.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-scripts": "4.0.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "immutability-helper": "3.1.1",
    "lodash": "4.17.21",
    "slugify": "1.6.0"
  }
}
